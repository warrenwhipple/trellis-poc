{
	"name": "@superset/db",
	"description": "Drizzle database schema",
	"version": "0.0.0",
	"private": true,
	"type": "module",
	"exports": {
		".": {
			"types": "./src/index.ts",
			"default": "./src/index.ts"
		},
		"./client": {
			"types": "./src/client.ts",
			"default": "./src/client.ts"
		},
		"./enums": {
			"types": "./src/schema/enums.ts",
			"default": "./src/schema/enums.ts"
		},
		"./schema": {
			"types": "./src/schema/index.ts",
			"default": "./src/schema/index.ts"
		}
	},
	"scripts": {
		"typecheck": "tsc --noEmit",
		"push": "drizzle-kit push",
		"migrate": "drizzle-kit migrate",
		"studio": "drizzle-kit studio"
	},
	"dependencies": {
		"@neondatabase/serverless": "1.0.1",
		"@superset/constants": "*",
		"@t3-oss/env-core": "^0.13.8",
		"dotenv": "^17.2.3",
		"drizzle-orm": "0.44.3",
		"drizzle-zod": "0.8.2",
		"zod": "^4.1.12"
	},
	"devDependencies": {
		"@superset/typescript": "*",
		"@types/node": "^24.9.1",
		"drizzle-kit": "0.31.1",
		"typescript": "^5.9.3"
	}
}
